@EVENT_VIDEO_DAY
#DIM VIEW
#DIM FAV
;-----------------------------------

SIF TARGET < 0 || TARGET > CHARANUM
	RETURN 0

;非公開ははじく
SIF CFLAG:497 == 0
	RETURN 0

VIEW = 0
FAV = 0

;評価ボーナス
VIEW += CFLAG:493 / 10

;マニアックさによるデメリット
IF CFLAG:494 > 0 && CFLAG:494 < 100
	VIEW *= 100 - CFLAG:494
	VIEW /= 100
	VIEW += 1
ENDIF

;ふぁぼは閲覧數の1%程度
FAV = (VIEW / 100) + RAND:2
;マニアックボーナス
FAV *= (CFLAG:494 / 2) + 1
;閲覧數以上のふぁぼは得られない
SIF FAV > VIEW
	FAV = VIEW

;加算
CFLAG:495 += VIEW
CFLAG:496 += FAV

IF CSTR:6 != ""
	PRINTL  
	PRINTFORML 錄製於水晶中的「%CSTR:6%」在黑市上流通了起來……
	IF FAV > 0
		PRINTFORMW 收到了{VIEW}點金錢，並收到了{FAV}封粉絲信。
	ELSE
		PRINTFORMW 收到了{VIEW}點金錢。
	ENDIF
	
	MONEY += VIEW
	EX_FLAG:4444 += VIEW
	
	PRINTL  
ENDIF

RETURN 0

