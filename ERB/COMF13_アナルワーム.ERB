;eraIM@Sから導入しました(eramaou)

;-------------------------------------------------
;アナルバイブ
;道具使用コマンド：調教対象のアナルをアナルバイブで刺激
;-------------------------------------------------
@COM13

IF TEQUIP:90
	PRINTL 肛門觸手
	;SAVESTR:22 = 肛交觸手
ELSE
	PRINTL 肛門蟲
	;SAVESTR:22 = 肛門蟲
ENDIF


IF DAY:2 == 15 && TALENT:191 == 0
	PRINTL *滿月是蠕蟲的產卵期，要繼續嗎？*
	PRINTL [0] 好的 [1] 算了
	INPUT
	SIF RESULT != 0
		RETURN 0
ENDIF

CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 60
LOSEBASE:1 += 150

SOURCE:14 = 300

;ABL:肛門感覺をみる
IF ABL:3 == 0
	SOURCE:2 = 80
	SOURCE:13 = 300
ELSEIF ABL:3 == 1
	SOURCE:2 = 250
	SOURCE:13 = 800
ELSEIF ABL:3 == 2
	SOURCE:2 = 600
	SOURCE:13 = 1400
ELSEIF ABL:3 == 3
	SOURCE:2 = 1000
	SOURCE:13 = 1800
ELSEIF ABL:3 == 4
	SOURCE:2 = 1300
	SOURCE:13 = 2100
ELSE
	SOURCE:2 = 1700
	SOURCE:13 = 2400
ENDIF

;EXP:肛門經驗をみる
IF EXP:1 < EXPLV:1
	TIMES SOURCE:2 , 0.50
	SOURCE:6 = 2000
	SOURCE:14 += 200
ELSEIF EXP:1 < EXPLV:2
	TIMES SOURCE:2 , 1.00
	SOURCE:6 = 300
	SOURCE:14 += 100
ELSEIF EXP:1 < EXPLV:3
	TIMES SOURCE:2 , 1.10
	SOURCE:6 = 50
	SOURCE:14 += 50
ELSEIF EXP:1 < EXPLV:4
	TIMES SOURCE:2 , 1.20
	SOURCE:6 = 10
ELSEIF EXP:1 < EXPLV:5
	TIMES SOURCE:2 , 1.40
	SOURCE:6 = 0
ELSE
	TIMES SOURCE:2 , 1.60
	SOURCE:6 = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:3 < PALAMLV:1
	TIMES SOURCE:2 , 0.40
	SOURCE:6 += 800
ELSEIF PALAM:3 < PALAMLV:2
	TIMES SOURCE:2 , 0.80
	SOURCE:6 += 500
ELSEIF PALAM:3 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	SOURCE:6 += 300
ELSEIF PALAM:3 < PALAMLV:4
	TIMES SOURCE:2 , 1.40
	SOURCE:6 += 120
ELSEIF PALAM:3 >= PALAMLV:4
	TIMES SOURCE:2 , 1.80
	SOURCE:6 += 100
ENDIF

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES SOURCE:2 , 0.80
ELSEIF PALAM:5 < PALAMLV:2
	TIMES SOURCE:2 , 0.90
ELSEIF PALAM:5 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES SOURCE:2 , 1.10
ELSEIF PALAM:5 >= PALAMLV:4
	TIMES SOURCE:2 , 1.20
ENDIF

;ABL:順從をみる
IF ABL:10 == 0
	TIMES SOURCE:2 , 0.80
	TIMES SOURCE:14 , 2.00
ELSEIF ABL:10 == 1
	TIMES SOURCE:2 , 0.90
	TIMES SOURCE:14 , 1.50
ELSEIF ABL:10 == 2
	TIMES SOURCE:14 , 1.00
ELSEIF ABL:10 == 3
	TIMES SOURCE:14 , 0.80
ELSEIF ABL:10 == 4
	TIMES SOURCE:14 , 0.60
ELSE
	TIMES SOURCE:14 , 0.30
ENDIF

;魁梧
SIF TALENT:99
	TIMES SOURCE:6 , 0.80
;嬌小
SIF TALENT:100
	TIMES SOURCE:6 , 2.00
;未熟
SIF TALENT:135
	TIMES SOURCE:6 , 2.00

;肛門敏感、鈍感をみる
;肛門自體のチェックは後でまとめてやる
IF TALENT:105
	TIMES SOURCE:6 , 1.50
	TIMES SOURCE:13 , 1.50
	TIMES SOURCE:14 , 1.50
ELSEIF TALENT:106
	TIMES SOURCE:6 , 0.60
	TIMES SOURCE:13 , 0.60
	TIMES SOURCE:14 , 0.60
ENDIF

IF EXP:0 == 0 && TALENT:30
	SOURCE:13 /= 3
ENDIF

;-------------------------------------------------
;経験上昇
;-------------------------------------------------
S = 0
;肛門感覺が高いほど肛門經驗が入る
IF ABL:3 <= 1
	S = 1
ELSEIF ABL:3 <= 4
	S = 2
ELSEIF ABL:3 <= 7
	S = 3
ELSE
	S = 4
ENDIF
EXP:1 += S
PRINTFORML 肛門經驗+{S}

;レズ・ホモ経験
IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +1
	EXP:40 += 1
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +1
	EXP:41 += 1
ENDIF

;-------------------------------------------------
;アナルバイブの著脫
;-------------------------------------------------
TEQUIP:13 = 1 - TEQUIP:13
SIF TEQUIP:90
	T = 0

;-------------------------------------------------
;汚れ（觸手）
;-------------------------------------------------
IF TEQUIP:90 && TEQUIP:13
	STAIN:4 |= 2
	STAIN:4 |= 4
ENDIF

RETURN 1

