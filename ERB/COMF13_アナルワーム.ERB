;魁梧
SIF TALENT:99
	TIMES LOCAL:2 , 0.80
;小柄體型
SIF TALENT:100
	TIMES LOCAL:2 , 2.00
;未熟
SIF TALENT:135
	TIMES SOURCE:6 , 2.00

;肛門敏感、鈍感をみる
;肛門自體のチェックは後でまとめてやる
IF TALENT:105
	TIMES SOURCE:6 , 1.50
	TIMES SOURCE:13 , 1.50
	TIMES SOURCE:14 , 1.50
ELSEIF TALENT:106
	TIMES SOURCE:6 , 0.60
	TIMES SOURCE:13 , 0.60
	TIMES SOURCE:14 , 0.60
ENDIF

SOURCE:2 += LOCAL:0
SOURCE:13 += LOCAL:1
SOURCE:6 += LOCAL:2

;處女で看重貞操
IF TALENT:0 && TALENT:30
	TIMES SOURCE:13 , 0.80
	TIMES SOURCE:14 , 0.50
ENDIF

;-------------------------------------------------
;経験上昇
;-------------------------------------------------
S = 0
;自慰実行時は経験＋１
SIF SELECTCOM == 3
	S = 1
;肛門感覺が高いほど肛門經驗が入る
IF ABL:3 <= 1
	S += 1
ELSEIF ABL:3 <= 4
	S += 2
ELSEIF ABL:3 <= 7
	S += 3
ELSE
	S += 4
ENDIF
EXP:1 += S
PRINTFORML 肛門經驗+{S}
S = 0

;-------------------------------------------------
;その他の処理
;-------------------------------------------------
SIF TEQUIP:90
	T += 1

