ELSEIF ABL:10 == 1
	TIMES LOCAL:0 , 0.90
	TIMES SOURCE:14 , 1.50
ELSEIF ABL:10 == 2
	TIMES SOURCE:14 , 1.00
ELSEIF ABL:10 == 3
	TIMES SOURCE:14 , 0.80
ELSEIF ABL:10 == 4
	TIMES SOURCE:14 , 0.60
ELSE
