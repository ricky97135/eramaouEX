;愛撫系フィルタが入ってるとダメ
SIF FLAG:25 & 1
	RETURN 0
;決闘中はダメ
SIF TEQUIP:55
	RETURN 0
RETURN 1
;-------------------------------------------------
;舔陰実行判定
;-------------------------------------------------
@COM_ABLE1
;愛撫系フィルタが入ってるとダメ
SIF FLAG:25 & 1
	RETURN 0
;対象が男人だとダメ
SIF TALENT:122
	RETURN 0
;觸手調教中はダメ
SIF TEQUIP:90
	RETURN 0
;決闘中はダメ
SIF TEQUIP:55
	RETURN 0
;パンツか上著下・ズボンタイプを著てるとダメ
SIF (CFLAG:40 & 17) && FLAG:37
	RETURN 0
;オムツをはいているとダメ
SIF CFLAG:42 == 69 && (CFLAG:40 & 64) && FLAG:37
	RETURN 0
;ズーコの著ぐるみを著てるとダメ
SIF CFLAG:42 == 11 && (CFLAG:40 & 64) && FLAG:37
	RETURN 0
;性器が精液、アナル、尿で汚れており、反感污臭で順從3以下のASSIの場合舔陰はできない（調教者が不怕臟ならＯＫ）
IF (STAIN:2 & 4 || STAIN:2 & 8 || STAIN:2 & 32) && ASSIPLAY
	SIF ABL:ASSI:0 <= 3 && TALENT:ASSI:62 && TALENT:ASSI:64 == 0
		RETURN 0
ENDIF
RETURN 1

;-------------------------------------------------
;肛門愛撫実行判定
;-------------------------------------------------
@COM_ABLE2
resetcolor
;愛撫系フィルタが入ってるとダメ
SIF FLAG:25 & 1
	RETURN 0
;決闘中はダメ
SIF TEQUIP:55
	RETURN 0
;獸姦PLAY中はダメ
SIF TEQUIP:89
	RETURN 0
;パンツか上著下・ズボンタイプを著てるとダメ
SIF (CFLAG:40 & 17) && FLAG:37
	RETURN 0
;オムツをはいているとダメ
SIF CFLAG:42 == 69 && (CFLAG:40 & 64) && FLAG:37
	RETURN 0
;ズーコの著ぐるみを著てるとダメ
SIF CFLAG:42 == 11 && (CFLAG:40 & 64) && FLAG:37
	RETURN 0
;主人プレイなら自動成功
IF ASSIPLAY == 0
	RETURN 1
;助手プレイの場合
ELSE
	;潤滑不足の場合はダメ
	IF  PALAM:3 < PALAMLV:2
		;助手が施虐狂ならOK
		IF TALENT:ASSI:83
			RETURN 1
		;助手が順從4以上かつ百合氣質4以上の場合はOK
		ELSEIF ABL:ASSI:10 <= 3 && ABL:ASSI:22 <= 3
			RETURN 1
		ENDIF
		RETURN 0
	ENDIF
ENDIF
RETURN 1

