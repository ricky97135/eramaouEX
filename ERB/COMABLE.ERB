@V_ABLE(ARG)
;ARGのV行為が可能かどうかを判定する関數です
;オトコだとダメ
SIF TALENT:(ARG):122
	RETURN 0
;未熟だとダメ（調教者がサドなら可）
SIF TALENT:(ARG):135 && !GETBIT(FLAG:700,4)
	RETURN 0
;処女だとダメ
SIF TALENT:(ARG):0
	RETURN 0
;貞操帯を付けているとダメ
SIF CFLAG:(ARG):42 == 79 && (CFLAG:(ARG):40 & 64) && FLAG:37
	RETURN 0
;貞操封印だとダメ
SIF TALENT:(ARG):273
	RETURN 0
RETURN 1

;-------------------------------------------------
;0-9 愛撫系コマンド
