	SIF CFLAG:41 == 0 && CFLAG:40 & 3
		CFLAG:41 = 1
;內衣也被脫掉了
ELSEIF (CFLAG:41 == 1 || CFLAG:41 == -1) && !(CFLAG:40 & 3)
	CFLAG:41 = 0
ENDIF
;ダメになった特別コスは削除
IF CFLAG:42
	SIF CFLAG:47 < 0
		CFLAG:42 = 0
ENDIF

RETURN 1

