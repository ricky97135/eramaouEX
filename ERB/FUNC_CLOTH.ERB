		RETURN 1
	ELSEIF (CFLAG:40 & 4) == 0 && (CFLAG:40 & 24)
		IF (CFLAG:40 & 2)
			PRINT 只穿運動短褲和胸罩
		ELSE
			PRINT 只穿一條運動短褲
		ENDIF
		RETURN 1
	ENDIF
ENDIF
;全身タイプで半脫ぎの場合の処理
IF CFLAG:41 >= 201 && CFLAG:41 <= 300
	IF (CFLAG:40 & 4) && (CFLAG:40 & 24) == 0
		CALL PRINT_CLOTHTYPE_MAIN2
		PRINT 的下半身被撕破了，
		IF (CFLAG:40 & 1)
			PRINTFORM 內褲完全暴露出來
		ELSE
			PRINT 下半身裸露
		ENDIF
		RETURN 1
	ELSEIF (CFLAG:40 & 4) == 0 && (CFLAG:40 & 24)
		CALL PRINT_CLOTHTYPE_MAIN2
		PRINT 的前襟撕裂了，
		IF (CFLAG:40 & 2)
			PRINT 上身只穿著胸罩
		ELSE
			IF TALENT:122 == 0 && TALENT:116 == 0 && (TALENT:109 == 0 || TALENT:132 == 0)
				PRINT 乳房外露
			ELSE
				PRINT 上半身裸露
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
ENDIF
;通常の衣服
IF (CFLAG:40 & 28)
	IF (CFLAG:40 & 4) && (CFLAG:40 & 24) == 0
		PRINT 只穿著
		CALL PRINT_CLOTHTYPE_MAIN2
		PRINT 的上衣，
		IF (CFLAG:40 & 1)
			PRINT 下身只有一條內褲
		ELSE
			PRINT 下半身裸露
		ENDIF
	ELSEIF (CFLAG:40 & 4) == 0 && (CFLAG:40 & 24)
		IF (CFLAG:40 & 2)
			PRINT 穿著胸罩，
		ELSE
			IF TALENT:122 == 0 && TALENT:116 == 0 && (TALENT:109 == 0 || TALENT:132 == 0)
				PRINT 乳房外露，
			ELSE
				PRINT 上半身裸露，
			ENDIF
		ENDIF
		PRINT 穿著
		CALL PRINT_CLOTHTYPE_MAIN2
		IF CFLAG:41 >= 1 && CFLAG:41 <= 100
			PRINT 的裙子
		ELSE
			PRINT 的下身
		ENDIF
	ELSE
		CALL PRINT_CLOTHTYPE_MAIN2
		SIF (CFLAG:40 & 64) == 0
			PRINT 的姿態
	ENDIF
ELSEIF (CFLAG:40 & 1) && (CFLAG:40 & 2)
	PRINT 內衣
	SIF (CFLAG:40 & 64) == 0
		PRINT 姿態
ELSEIF (CFLAG:40 & 1)
	PRINTFORM 只穿著一條內褲
ELSEIF (CFLAG:40 & 2)
	PRINT 只穿著胸罩，下身裸露
ELSE
