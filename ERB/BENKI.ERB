@SELECT_BENKI_MENU(ARG, ARGS)
#DIM ANSWER
#DIM DICE
;----------------------------------------------
;ARG  參照キャラ
;ARGS 參照內容
;肉便器のアクションを振り分ける
;ANSWER = 調教コマンドと同じ

;デフォは愛撫
ANSWER = 0
DICE = 2

IF ARGS == "戦闘"
	;上書きする可能性のある後ろの判定ほど低確率に
	
	;技巧2以上で手淫分岐
	IF ABL:(ARG):12 >= 2 && RAND:DICE == 0
		ANSWER = 30
		DICE += 1
	ENDIF
	
	;奉仕技術2以上でフェラ分岐
	IF ABL:(ARG):13 >= 2 && RAND:DICE == 0
		ANSWER = 31
		DICE += 1
	ENDIF
	
	;露出癖2以上で自慰分岐
	IF ABL:(ARG):17 >= 2 && RAND:DICE == 0
		ANSWER = 3
		DICE += 1
	ENDIF
	
	;奉仕精神2以上でアナル奉仕分岐
	IF ABL:(ARG):16 >= 2 && RAND:DICE == 0
		ANSWER = 37
		DICE += 1
	ENDIF
	
	;V感覚2以上で正常位分岐
	CALL V_ABLE,ARG
	IF ABL:(ARG):2 >= 2 && RESULT == 1 && RAND:DICE == 0
		ANSWER = 20
		DICE += 1
	ENDIF
	
	;V感覚2以上で後背位分岐
	CALL V_ABLE,ARG
	IF ABL:(ARG):2 >= 2 && RESULT == 1 && RAND:DICE == 0
		ANSWER = 21
		DICE += 1
	ENDIF
	
	;A感覚2以上で正常位アナル分岐
	IF ABL:(ARG):3 >= 2 && RAND:DICE == 0
		ANSWER = 26
		DICE += 1
	ENDIF
	
	;A感覚2以上で後背位アナル分岐
	IF ABL:(ARG):3 >= 2 && RAND:DICE == 0
		ANSWER = 27
		DICE += 1
	ENDIF
	
ENDIF

RETURN ANSWER

