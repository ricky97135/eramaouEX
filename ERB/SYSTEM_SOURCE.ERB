;-------------------------------------------------
@LOSELIFE_BAR
SIF MAXBASE:0 <= 0
	RETURN 0

PRINT  體力
A = (BASE:0*32) / MAXBASE:0
SIF BASE:0 < 0
	A = 0
B = (LOSEBASE:0*32) / MAXBASE:0 + 1
SIF BASE:0 < 0
	B = 0
PRINT [
IF A > 0
	REPEAT A
		PRINT =
	REND
ENDIF
IF B > 0
	REPEAT B
		PRINT -
	REND
ENDIF
IF 32 - (A+B) > 0
	REPEAT 32 - (A+B)
		PRINT .
	REND
ENDIF
PRINT ]

