;eraIM@Sから導入しました(eramaou)

;-------------------------------------------------
;背後位・打屁股
;セックス系コマンド：背後位と打屁股の合わせ技。抖M氣質による補正が高い
;-------------------------------------------------
@COM132

SELECTCOM = 132

;處女を奪うかどうかの確認
CALL CONFIRM_LOST_VIRGIN
SIF RESULT == 0
	RETURN 0

;安全套を使うかの確認
CALL CONFIRM_CONDOM
SIF !RESULT
	RETURN 0
	
PRINTL 背後位・打屁股
;SAVESTR:22 = 背後位・打屁股
CALL TRAIN_MESSAGE_B

;私處經驗を伴うコマンドのフラグ(處女膜再生対応)
TFLAG:19 = 1
SIF TALENT:85 && ASSIPLAY == 0 && EXP:0 == 0 && TEQUIP:89 == 0
	TFLAG:20 = 1

;調教者の射精チェック
CALL COM_EJAC_PLAYER_SEX

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 100
LOSEBASE:1 += 120

SOURCE:10 = 800
SOURCE:12 = 800
SOURCE:13 = 800
SOURCE:14 = 500

;ABL:私處感覺をみる
IF ABL:2 == 0
	SOURCE:1 = 40
	SOURCE:3 = 50
ELSEIF ABL:2 == 1
	SOURCE:1 = 150
	SOURCE:3 = 150
ELSEIF ABL:2 == 2
	SOURCE:1 = 400
	SOURCE:3 = 250
ELSEIF ABL:2 == 3
	SOURCE:1 = 1000
	SOURCE:3 = 350
ELSEIF ABL:2 == 4
	SOURCE:1 = 1700
	SOURCE:3 = 600
ELSE
	SOURCE:1 = 2200
	SOURCE:3 = 850
ENDIF

;EXP:私處經驗をみる
IF EXP:0 < EXPLV:1
	TIMES SOURCE:1 , 0.20
	SOURCE:6 = 5000

	IF ASSIPLAY && TALENT:PLAYER:122 == 0
		EXP:50 += 1
		PRINTFORML %EXPNAME:50%＋１
	ENDIF
ELSEIF EXP:0 < EXPLV:2
	TIMES SOURCE:1 , 0.60
	SOURCE:6 = 220
ELSEIF EXP:0 < EXPLV:3
	TIMES SOURCE:1 , 1.00
	SOURCE:6 = 30
ELSEIF EXP:0 < EXPLV:4
	TIMES SOURCE:1 , 1.20
	SOURCE:6 = 5
ELSEIF EXP:0 < EXPLV:5
	TIMES SOURCE:1 , 1.30
	SOURCE:6 = 0
ELSE
	TIMES SOURCE:1 , 1.80
	SOURCE:6 = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:3 < PALAMLV:1
	TIMES SOURCE:1 , 0.10
	SOURCE:6 += 900
	TIMES SOURCE:6 , 3.00
ELSEIF PALAM:3 < PALAMLV:2
	TIMES SOURCE:1 , 0.40
	SOURCE:6 += 250
	TIMES SOURCE:6 , 1.00
ELSEIF PALAM:3 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:6 , 0.50
ELSEIF PALAM:3 < PALAMLV:4
	TIMES SOURCE:1 , 1.40
	TIMES SOURCE:6 , 0.20
ELSEIF PALAM:3 >= PALAMLV:4
	TIMES SOURCE:1 , 1.80
	TIMES SOURCE:6 , 0.10
ENDIF

;調教者が扶她
IF ASSIPLAY
	SIF TALENT:ASSI:121
		TIMES SOURCE:1 , 2.50
ENDIF

;魁梧
SIF TALENT:99
	TIMES SOURCE:6 , 1.80
;小柄體形
SIF TALENT:100
	TIMES SOURCE:6 , 2.00

;看重貞操
IF TALENT:30
	IF EXP:0 == 0
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 10000
	ELSE
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 1000
	ENDIF
;看輕貞操
ELSEIF TALENT:31
	IF EXP:0 == 0
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 300
	ENDIF
ELSE
	IF EXP:0 == 0
		SOURCE:15 = 3000
	ENDIF
ENDIF

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES SOURCE:1 , 0.60
	TIMES SOURCE:3 , 0.30
ELSEIF PALAM:5 < PALAMLV:2
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:3 , 0.60
ELSEIF PALAM:5 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:3 , 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:3 , 1.50
ELSEIF PALAM:5 >= PALAMLV:4
	TIMES SOURCE:1 , 1.50
	TIMES SOURCE:3 , 1.80
ENDIF

;ABL:順從をみる
IF ABL:10 == 0
	TIMES SOURCE:1 , 0.50
	TIMES SOURCE:3 , 0.60
	TIMES SOURCE:15 , 2.00
ELSEIF ABL:10 == 1
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:3 , 0.80
	TIMES SOURCE:15 , 1.50
ELSEIF ABL:10 == 2
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:3 , 1.00
	TIMES SOURCE:15 , 1.00
ELSEIF ABL:10 == 3
	TIMES SOURCE:1 , 1.30
	TIMES SOURCE:3 , 1.20
	TIMES SOURCE:15 , 0.80
ELSEIF ABL:10 == 4
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:3 , 1.40
	TIMES SOURCE:15 , 0.60
ELSE
	TIMES SOURCE:1 , 2.00
	TIMES SOURCE:3 , 1.60
	TIMES SOURCE:15 , 0.30
ENDIF

;PALAM:苦痛をみる
IF PALAM:9 < PALAMLV:1
	SOURCE:6 = 300
ELSEIF PALAM:9 < PALAMLV:2
	SOURCE:6 = 500
ELSEIF PALAM:9 < PALAMLV:3
	SOURCE:6 = 800
ELSEIF PALAM:9 < PALAMLV:4
	SOURCE:6 = 1200
ELSEIF PALAM:9 >= PALAMLV:4
	SOURCE:6 = 1800
ENDIF

;ABL:抖M氣質をみる
IF ABL:21 == 0
	TIMES SOURCE:1 , 0.60
	TIMES SOURCE:10 , 0.80
	TIMES SOURCE:13 , 0.80
ELSEIF ABL:21 == 1
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:10 , 1.00
	TIMES SOURCE:13 , 1.00
ELSEIF ABL:21 == 2
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:10 , 1.20
	TIMES SOURCE:13 , 1.50
ELSEIF ABL:21 == 3
	TIMES SOURCE:1 , 1.10
	TIMES SOURCE:10 , 1.40
	TIMES SOURCE:13 , 3.00
ELSEIF ABL:21 == 4
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:10 , 2.00
	TIMES SOURCE:13 , 5.00
ELSE
	TIMES SOURCE:1 , 1.30
	TIMES SOURCE:10 , 3.00
	TIMES SOURCE:13 , 8.00
ENDIF

;セックス後の処理
CALL COM_AFTER_VAGINA_SEX

RETURN 1
;
