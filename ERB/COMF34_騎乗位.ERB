;eraIM@Sから導入しました(eramaou)

;-------------------------------------------------
;騎乘位
;奉仕系コマンド：調教対象が調教者のペニスをヴァギナで刺激
;-------------------------------------------------
@COM34

;-------------------------------------------------
;跳轉到進階指令
;-------------------------------------------------
LOCAL = 34
CALL GET_ADV_COM,LOCAL
SIF RESULT != LOCAL
	JUMPFORM COM{RESULT}

FLAG:71 = 0

PRINTL 騎乘位

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL COM34_CHECK
SIF !RESULT
	RETURN 0

CALL COM34_EXE_DETER
SIF !RESULT
	RETURN 0

CALL COM34_SRC

CALL COM34_POST

RETURN 1



@COM34_CHECK
;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(順從が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:慾望
IF ABL:11
	SIF S
		PRINT  + 
	A += ABL:11 * 3
	PRINTS ABLNAME:11
	PRINTV 'LV,ABL:11,'(,ABL:11 * 3,')
	S = 1
ENDIF
;ABL:私處感覺
IF ABL:2
	SIF S
		PRINT  + 
	A += ABL:2 * 2
	PRINTS ABLNAME:2
	PRINTV 'LV,ABL:2,'(,ABL:2 * 2,')
	S = 1
ENDIF
;ABL:侍奉精神
IF ABL:16
	SIF S
		PRINT  + 
	A += ABL:16 * 4
	PRINTS ABLNAME:16
	PRINTV 'LV,ABL:16,'(,ABL:16 * 4,')
	S = 1
ENDIF
;ABL:精液中毒
IF ABL:32
	SIF S
		PRINT  + 
	A += ABL:32 * 1
	PRINTS ABLNAME:32
	PRINTV 'LV,ABL:32,'(,ABL:32 * 1,')
	S = 1
ENDIF

IF MARK:1
	SIF S
		PRINT  + 
	A += MARK:1 * 3
	PRINTS MARKNAME:1
	PRINTV 'LV,MARK:1,'(,MARK:1 * 3,')
	S = 1
ENDIF

;PALAM:潤滑（の不足）
IF PALAM:3 < PALAMLV:3
	PRINT  - 
	A -= 5
	PRINT 潤滑不足
	PRINTV '(,5,')
	S = 1
ENDIF
;PALAM:欲情
IF PALAM:5 < PALAMLV:1
	L = 0
ELSEIF PALAM:5 < PALAMLV:2
	L = 1
ELSEIF PALAM:5 < PALAMLV:3
	L = 2
ELSEIF PALAM:5 < PALAMLV:4
	L = 3
ELSEIF PALAM:5 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	SIF S
		PRINT  + 
	A += L * 3
	PRINTS PALAMNAME:5
	PRINTV 'LV,L,'(,L * 3,')
	S = 1
ENDIF

;害羞
IF TALENT:35
	PRINT  - 
	A -= 2
	PRINTS TALENTNAME:35
	PRINTV '(,2,')
	S = 1
ENDIF
;否定快感
IF TALENT:71
	PRINT  - 
	A -= 5
	PRINTS TALENTNAME:71
	PRINTV '(,5,')
	S = 1
ENDIF
;討厭男人
IF TALENT:82 && TALENT:PLAYER:122
	PRINT  - 
	A -= 12
	PRINTS TALENTNAME:82
	PRINTV '(,12,')
	S = 1
ENDIF
;愛慕
IF TALENT:85 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:85
	PRINTV '(,5,')
	S = 5
ENDIF

;性愛狂
IF TALENT:75
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:75
	PRINTV '(,5,')
	S = 5
ENDIF

;處女
IF TALENT:0
	PRINT  - 
	A -= 20
	PRINTS TALENTNAME:0
	PRINTV '(,20,')
	S = 1
ELSEIF EXP:0 < EXPLV:2
	PRINT  - 
	A -= 5
	PRINTS EXPNAME:0
	PRINT 不足
	PRINTV '(,5,')
	S = 1
ENDIF

;調教者が扶她
IF TALENT:PLAYER:121
	SIF S
		PRINT  + 
	A += 4
	PRINTS TALENTNAME:121
	PRINTV '(,4,')
	S = 1
ENDIF

;しあわせ草
IF TEQUIP:21
	SIF S
		PRINT  + 
	A += 6
	PRINTS ITEMNAME:26
	PRINTV '(,6,')
	S = 1
ENDIF

;獸姦
IF TEQUIP:89 && TALENT:136 == 0
	SIF S
		PRINT  - 
	A -= 20
	PRINTS ITEMNAME:22
	PRINTV '(,20,')
	S = 1
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:2 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:2 & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:2 & 8
	Y += 7
;母乳の汚れ
SIF STAIN:PLAYER:2 & 16
	Y += 1
;尿の汚れ
SIF STAIN:PLAYER:4 & 32
	Y += 3

;獸姦の場合は汚れ7で固定
SIF TEQUIP:89
	Y = 7

SIF TALENT:61
	Y /= 3
SIF TALENT:62
	Y *= 2

;騎乘位では汚れの影響が少なめ
Y /= 3

;汚れあり
IF Y
	PRINT  - 
	A -= Y
	;不怕污臭
	IF TALENT:61
		PRINT 臟、
		PRINTS TALENTNAME:61
	ELSEIF TALENT:62
		PRINT 臟、
		PRINTS TALENTNAME:62
	ELSE
		PRINT 臟
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF

;合計を表示(24以上で実行)
PRINT  = 
PRINTV A

V = 24
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 實行值
PRINTV V

WAIT

;実行できない
SIF A < V
	RETURN 0

RETURN 1
	
@COM34_EXE_DETER
;-------------------------------------------------
;実行決定
;-------------------------------------------------
;處女を奪うかどうかの確認
CALL CONFIRM_LOST_VIRGIN
SIF RESULT == 0
	RETURN 0

;安全套を使うかの確認
CALL CONFIRM_CONDOM
SIF !RESULT
	RETURN 0
	
;SAVESTR:22 = 騎乘位
CALL TRAIN_MESSAGE_B

;私處經驗を伴うコマンドのフラグ(處女膜再生対応)
TFLAG:19 = 1
SIF ASSIPLAY == 0 && EXP:0 == 0 && TEQUIP:89 == 0
	TFLAG:20 = 1

RETURN 1
	
@COM34_SRC
;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 60
LOSEBASE:1 += 150

SOURCE:12 = 900

;上のほうで計算した汚れデータ
SOURCE:8 = Y*10 + 60

;ABL:侍奉精神をみる
IF ABL:16 == 0
	SOURCE:4 = 200
	SOURCE:5 = 50
	SOURCE:14 = 300
	TIMES SOURCE:8 , 4.00
ELSEIF ABL:16 == 1
	SOURCE:4 = 250
	SOURCE:5 = 200
	SOURCE:14 = 100
	TIMES SOURCE:8 , 2.50
ELSEIF ABL:16 == 2
	SOURCE:4 = 350
	SOURCE:5 = 550
	SOURCE:14 = 30
	TIMES SOURCE:8 , 1.50
ELSEIF ABL:16 == 3
	SOURCE:4 = 450
	SOURCE:5 = 900
	SOURCE:14 = 0
	TIMES SOURCE:8 , 1.00
ELSEIF ABL:16 == 4
	SOURCE:4 = 600
	SOURCE:5 = 1500
	SOURCE:14 = 0
	TIMES SOURCE:8 , 0.50
ELSE
	SOURCE:4 = 750
	SOURCE:5 = 2200
	SOURCE:14 = 0
	TIMES SOURCE:8 , 0.10
ENDIF

;ABL:私處感覺をみる
IF ABL:2 == 0
	SOURCE:1 = 20
	SOURCE:3 = 25
	TIMES SOURCE:4 , 0.50
ELSEIF ABL:2 == 1
	SOURCE:1 = 75
	SOURCE:3 = 75
	TIMES SOURCE:4 , 0.80
ELSEIF ABL:2 == 2
	SOURCE:1 = 200
	SOURCE:3 = 125
	TIMES SOURCE:4 , 1.00
ELSEIF ABL:2 == 3
	SOURCE:1 = 500
	SOURCE:3 = 175
	TIMES SOURCE:4 , 1.20
ELSEIF ABL:2 == 4
	SOURCE:1 = 850
	SOURCE:3 = 300
	TIMES SOURCE:4 , 1.50
ELSE
	SOURCE:1 = 1100
	SOURCE:3 = 425
	TIMES SOURCE:4 , 2.00
ENDIF

;EXP:私處經驗をみる
IF EXP:0 < EXPLV:1
	TIMES SOURCE:1 , 0.20
	SOURCE:6 = 5000
	SOURCE:12 = 12000
	SOURCE:14 = 120000
ELSEIF EXP:0 < EXPLV:2
	TIMES SOURCE:1 , 0.60
	SOURCE:6 = 220
ELSEIF EXP:0 < EXPLV:3
	TIMES SOURCE:1 , 1.00
	SOURCE:6 = 30
ELSEIF EXP:0 < EXPLV:4
	TIMES SOURCE:1 , 1.20
	SOURCE:6 = 5
ELSEIF EXP:0 < EXPLV:5
	TIMES SOURCE:1 , 1.40
ELSE
	TIMES SOURCE:1 , 1.50
ENDIF


;PALAM:潤滑
IF PALAM:3 < PALAMLV:1
	TIMES SOURCE:1 , 0.20
	SOURCE:6 += 900
	TIMES SOURCE:6 , 3.00
	SOURCE:12 += 2000
ELSEIF PALAM:3 < PALAMLV:2
	TIMES SOURCE:1 , 0.60
	SOURCE:6 += 250
	TIMES SOURCE:6 , 1.00
	SOURCE:12 += 400
ELSEIF PALAM:3 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:6 , 0.50
ELSEIF PALAM:3 < PALAMLV:4
	TIMES SOURCE:1 , 1.30
	TIMES SOURCE:6 , 0.30
ELSEIF PALAM:3 < PALAMLV:5
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:6 , 0.20
ELSE
	TIMES SOURCE:1 , 2.00
	TIMES SOURCE:6 , 0.10
ENDIF

;魁梧
SIF TALENT:99
	TIMES SOURCE:6 , 0.80
;小柄體形
SIF TALENT:100
	TIMES SOURCE:6 , 2.00
;未熟
SIF TALENT:135
	TIMES SOURCE:6 , 4.00

;看重貞操
IF TALENT:30
	IF EXP:0 == 0
		TIMES SOURCE:3 , 0.60
		TIMES SOURCE:14, 15.00
		SOURCE:15 = 10000
	ELSE
		TIMES SOURCE:3 , 0.60
		TIMES SOURCE:14, 15.00
		SOURCE:15 = 1000
	ENDIF
;看輕貞操
ELSEIF TALENT:31
	IF EXP:0 == 0
		TIMES SOURCE:14, 0.50
		SOURCE:15 = 300
	ELSE
		TIMES SOURCE:14, 0.50
	ENDIF
ELSE
	IF EXP:0 == 0
		SOURCE:15 = 3000
	ENDIF
ENDIF

;PALAM:欲情
IF PALAM:5 < PALAMLV:1
	TIMES SOURCE:1 , 0.60
	TIMES SOURCE:3 , 0.30
ELSEIF PALAM:5 < PALAMLV:2
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:3 , 0.60
ELSEIF PALAM:5 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:3 , 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:3 , 1.50
ELSEIF PALAM:5 < PALAMLV:5
	TIMES SOURCE:1 , 1.40
	TIMES SOURCE:3 , 1.80
ELSE
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:3 , 2.20
ENDIF

;ABL:順從をみる
IF ABL:10 == 0
	TIMES SOURCE:1 , 0.50
	TIMES SOURCE:3 , 0.60
	TIMES SOURCE:15 , 2.00
	SOURCE:13 += 700
ELSEIF ABL:10 == 1
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:3 , 0.80
	TIMES SOURCE:15 , 1.50
	SOURCE:13 += 500
ELSEIF ABL:10 == 2
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:3 , 1.00
	TIMES SOURCE:15 , 1.00
	SOURCE:13 += 300
ELSEIF ABL:10 == 3
	TIMES SOURCE:1 , 1.30
	TIMES SOURCE:3 , 1.20
	TIMES SOURCE:15 , 0.80
	SOURCE:13 += 100
ELSEIF ABL:10 == 4
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:3 , 1.40
	TIMES SOURCE:15 , 0.60
	SOURCE:13 += 30
ELSE
	TIMES SOURCE:1 , 2.00
	TIMES SOURCE:3 , 1.60
	TIMES SOURCE:15 , 0.30
	SOURCE:13 += 0
ENDIF

;ABL:技巧をみる
IF ABL:12 == 0
	TIMES SOURCE:1 , 0.50
	TIMES SOURCE:3 , 0.60
ELSEIF ABL:12 == 1
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:3 , 0.80
ELSEIF ABL:12 == 2
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:3 , 1.00
ELSEIF ABL:12 == 3
	TIMES SOURCE:1 , 1.30
	TIMES SOURCE:3 , 1.20
ELSEIF ABL:12 == 4
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:3 , 1.40
ELSE
	TIMES SOURCE:1 , 2.00
	TIMES SOURCE:3 , 1.60
ENDIF

;調教者の射精チェック
CALL COM_EJAC_PLAYER_SEX

;射精している
IF E
	TIMES SOURCE:4 , 3.00
	TIMES SOURCE:5 , 5.00
	TIMES SOURCE:13 , 1.50

	;ABL:精液中毒をみる
	IF ABL:32 == 0
		SOURCE:7 = 0
	ELSEIF ABL:32 == 1
		SOURCE:7 = 300
	ELSEIF ABL:32 == 2
		SOURCE:7 = 1000
	ELSEIF ABL:32 == 3
		SOURCE:7 = 1500
	ELSEIF ABL:32 == 4
		SOURCE:7 = 2200
	ELSE
		SOURCE:7 = 3000
	ENDIF
ENDIF

;セックス後の処理
CALL COM_AFTER_VAGINA_SEX

;調教者が扶她
IF TALENT:PLAYER:121
	SOURCE:13 /= 2
ENDIF


@COM34_POST
TFLAG:100 = 1

IF EXP:0 <= 1
	;屈服刻印３に相當
	TFLAG:200 = 3
ELSE
	;屈服刻印２に相當
	TFLAG:200 = 2
ENDIF


RETURN 1
