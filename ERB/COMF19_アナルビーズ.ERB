;-------------------------------------------------
;肛珠
;道具使用コマンド：調教対象のアナルを肛珠で刺激
;-------------------------------------------------
@COM19

PRINTL 肛珠
;SAVESTR:22 = 肛珠
CALL TRAIN_MESSAGE_B

LOSEBASE:0 += 60
LOSEBASE:1 += 150

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
;ABL:肛門感覺をみる
IF ABL:3 == 0
	SOURCE:2 = 80
	SOURCE:13 = 300
ELSEIF ABL:3 == 1
	SOURCE:2 = 250
	SOURCE:13 = 800
ELSEIF ABL:3 == 2
	SOURCE:2 = 600
	SOURCE:13 = 1400
ELSEIF ABL:3 == 3
	SOURCE:2 = 1000
	SOURCE:13 = 1800
ELSEIF ABL:3 == 4
	SOURCE:2 = 1300
	SOURCE:13 = 2100
ELSE
	SOURCE:2 = 1700
	SOURCE:13 = 2400
ENDIF

;EXP:肛門經驗をみる
IF EXP:1 < EXPLV:1
	TIMES SOURCE:2 , 0.50
	SOURCE:6 = 2000
ELSEIF EXP:1 < EXPLV:2
	TIMES SOURCE:2 , 1.00
	SOURCE:6 = 300
ELSEIF EXP:1 < EXPLV:3
	TIMES SOURCE:2 , 1.10
	SOURCE:6 = 50
ELSEIF EXP:1 < EXPLV:4
	TIMES SOURCE:2 , 1.20
	SOURCE:6 = 10
ELSEIF EXP:1 < EXPLV:5
	TIMES SOURCE:2 , 1.40
	SOURCE:6 = 0
ELSE
	TIMES SOURCE:2 , 1.60
	SOURCE:6 = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:3 < PALAMLV:1
	TIMES SOURCE:2 , 0.40
	SOURCE:6 += 1200
ELSEIF PALAM:3 < PALAMLV:2
	TIMES SOURCE:2 , 0.80
	SOURCE:6 += 700
ELSEIF PALAM:3 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	SOURCE:6 += 400
ELSEIF PALAM:3 < PALAMLV:4
	TIMES SOURCE:2 , 1.40
	SOURCE:6 += 150
ELSEIF PALAM:3 >= PALAMLV:4
	TIMES SOURCE:2 , 1.80
	SOURCE:6 += 100
ENDIF

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES SOURCE:2 , 0.80
ELSEIF PALAM:5 < PALAMLV:2
	TIMES SOURCE:2 , 0.90
ELSEIF PALAM:5 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES SOURCE:2 , 1.10
ELSEIF PALAM:5 >= PALAMLV:4
	TIMES SOURCE:2 , 1.20
ENDIF

;ABL:順從をみる
IF ABL:10 == 0
	TIMES SOURCE:2 , 0.80
ELSEIF ABL:10 == 1
	TIMES SOURCE:2 , 0.90
ELSEIF ABL:10 == 2
	TIMES SOURCE:2 , 1.00
ELSEIF ABL:10 == 3
	TIMES SOURCE:2 , 1.10
ELSEIF ABL:10 == 4
	TIMES SOURCE:2 , 1.20
ELSE
	TIMES SOURCE:2 , 1.30
ENDIF

;魁梧
SIF TALENT:99
	TIMES SOURCE:6 , 0.80
;嬌小
SIF TALENT:100
	TIMES SOURCE:6 , 2.00
;未熟
SIF TALENT:135
	TIMES SOURCE:6 , 2.00

;肛門敏感、鈍感をみる
;肛門自體のチェックは後でまとめてやる
IF TALENT:105
	TIMES SOURCE:6 , 1.50
	TIMES SOURCE:13 , 1.50
	TIMES SOURCE:14 , 1.50
ELSEIF TALENT:106
	TIMES SOURCE:6 , 0.60
	TIMES SOURCE:13 , 0.60
	TIMES SOURCE:14 , 0.60
ENDIF

IF EXP:0 == 0 && TALENT:30
	SOURCE:13 /= 3
ENDIF

SIF TEQUIP:19 == 0
	TIMES SOURCE:2 , 0.80

SIF TEQUIP:19 == 1
	TIMES SOURCE:2 , 3.00

;-------------------------------------------------
;経験上昇
;-------------------------------------------------
IF TEQUIP:19 == 0
EXP:1 += 1
PRINTL 肛門經驗＋１
ELSEIF TEQUIP:19 == 1
EXP:1 += 2
PRINTL 肛門經驗＋２
ENDIF

;-------------------------------------------------
;肛珠の著脫
;-------------------------------------------------
TEQUIP:19 = 1 - TEQUIP:19

RETURN 1

;-------------------------------------------------
;-------------------------------------------------
;肛珠
;-------------------------------------------------
@EQUIP_COM19

