ELSEIF ABL:10 == 1
	TIMES A , 0.90
ELSEIF ABL:10 == 2
	TIMES A , 1.00
ELSEIF ABL:10 == 3
	TIMES A , 1.10
ELSEIF ABL:10 == 4
	TIMES A , 1.20
ELSE
	TIMES A , 1.30
ENDIF

;魁梧
SIF TALENT:99
	TIMES C , 0.80
;嬌小
SIF TALENT:100
	TIMES C , 1.50
;未熟
SIF TALENT:135
	TIMES C , 2.00

;肛門敏感、鈍感をみる
;肛門自體のチェックは後でまとめてやる
IF TALENT:105
	TIMES SOURCE:6 , 1.50
	TIMES SOURCE:13 , 1.50
	TIMES SOURCE:14 , 1.50
ELSEIF TALENT:106
	TIMES SOURCE:6 , 0.60
	TIMES SOURCE:13 , 0.60
	TIMES SOURCE:14 , 0.60
ENDIF

SOURCE:2 += A
SOURCE:13 += B
SOURCE:6 += C

;處女で看重貞操
IF EXP:0 == 0 && TALENT:30
	SOURCE:13 /= 3
ENDIF


;-------------------------------------------------
;経験上昇
;-------------------------------------------------
EXP:1 += 1
PRINTL 肛門經驗＋１

IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +1
	EXP:40 += 1
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +1
	EXP:41 += 1
ENDIF

SIF TEQUIP:90
	T += 1

RETURN 1
