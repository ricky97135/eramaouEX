;eraIM@Sから導入しました(eramaou)

;-------------------------------------------------
;背面座位
;セックス系コマンド：調教者のペニスを調教対象のヴァギナに挿入
;正常位に比べて露出と情愛が低いが、調教対象の體力消耗が少ない
;-------------------------------------------------
@COM23
;處女を奪うかどうかの確認
CALL CONFIRM_LOST_VIRGIN
SIF RESULT == 0
	RETURN 0

;安全套を使うかの確認
CALL CONFIRM_CONDOM
SIF !RESULT
	RETURN 0
	

;-------------------------------------------------
;跳轉到進階指令
;-------------------------------------------------
LOCAL = 23
CALL GET_ADV_COM,LOCAL
SIF RESULT != LOCAL
	JUMPFORM COM{RESULT}

	
PRINTL 背面座位
;SAVESTR:22 = 背面座位
CALL TRAIN_MESSAGE_B

;私處經驗を伴うコマンドのフラグ(處女膜再生対応)
TFLAG:19 = 1
SIF TALENT:85 && ASSIPLAY == 0 && EXP:0 == 0
	TFLAG:20 = 1

;調教者の射精チェック
CALL COM_EJAC_PLAYER_SEX

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 20
LOSEBASE:1 += 150

SOURCE:12 = 200

;ABL:私處感覺をみる
IF ABL:2 == 0
	SOURCE:1 = 50
	SOURCE:3 = 50

ELSEIF ABL:2 == 1
	SOURCE:1 = 150
	SOURCE:3 = 100
ELSEIF ABL:2 == 2
	SOURCE:1 = 300
	SOURCE:3 = 200
ELSEIF ABL:2 == 3
	SOURCE:1 = 600
	SOURCE:3 = 300
ELSEIF ABL:2 == 4
	SOURCE:1 = 1000
	SOURCE:3 = 500
ELSE
	SOURCE:1 = 1500
	SOURCE:3 = 700
ENDIF

;EXP:私處經驗をみる
IF EXP:0 < EXPLV:1
	TIMES SOURCE:1 , 0.20
	SOURCE:6 = 3000

	IF ASSIPLAY && TALENT:PLAYER:122 == 0
		EXP:50 += 1
		PRINTFORML %EXPNAME:50%＋１
	ENDIF
ELSEIF EXP:0 < EXPLV:2
	TIMES SOURCE:1 , 0.60
	SOURCE:6 = 240
ELSEIF EXP:0 < EXPLV:3
	TIMES SOURCE:1 , 1.00
	SOURCE:6 = 30
ELSEIF EXP:0 < EXPLV:4
	TIMES SOURCE:1 , 1.20
	SOURCE:6 = 5
ELSEIF EXP:0 < EXPLV:5
	TIMES SOURCE:1 , 1.40
	SOURCE:6 = 0
ELSE
	TIMES SOURCE:1 , 1.60
	SOURCE:6 = 0
ENDIF

;ABL:乳房感覺をみる
IF ABL:1 == 0
	SOURCE:17 = 50
	SOURCE:3 += 50
ELSEIF ABL:1 == 1
	SOURCE:17 = 200
	SOURCE:3 += 200
ELSEIF ABL:1 == 2
	SOURCE:17 = 500
	SOURCE:3 += 400
ELSEIF ABL:1 == 3
	SOURCE:17 = 800
	SOURCE:3 += 600
ELSEIF ABL:1 == 4
	SOURCE:17 = 1300
	SOURCE:3 += 1000
ELSE
	SOURCE:17 = 1800
	SOURCE:3 += 1400
ENDIF

;ABL:陰蒂感覺をみる
IF ABL:0 == 0
	SOURCE:0 = 40
ELSEIF ABL:0 == 1
	SOURCE:0 = 160
ELSEIF ABL:0 == 2
	SOURCE:0 = 500
ELSEIF ABL:0 == 3
	SOURCE:0 = 900
ELSEIF ABL:0 == 4
	SOURCE:0 = 1400
ELSE
	SOURCE:0 = 2100
ENDIF

;PALAM:潤滑をみる
IF PALAM:3 < PALAMLV:1
	TIMES SOURCE:1 , 0.40
	SOURCE:6 += 600
	TIMES SOURCE:6 , 2.60
ELSEIF PALAM:3 < PALAMLV:2
	TIMES SOURCE:1 , 0.70
	SOURCE:6 += 180
	TIMES SOURCE:6 , 1.00
ELSEIF PALAM:3 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:6 , 0.50
ELSEIF PALAM:3 < PALAMLV:4
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:6 , 0.20
ELSEIF PALAM:3 >= PALAMLV:4
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:6 , 0.10
ENDIF

;調教者が扶她
IF ASSIPLAY
	SIF TALENT:ASSI:121
		TIMES SOURCE:1 , 2.50
ENDIF

;魁梧
SIF TALENT:99
	TIMES SOURCE:6 , 0.80
;小柄體形
SIF TALENT:100
	TIMES SOURCE:6 , 2.00
;未熟
SIF TALENT:135
	TIMES SOURCE:6 , 4.00

;看重貞操
IF TALENT:30
	IF EXP:0 == 0
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 10000
	ELSE
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 1000
	ENDIF
;看輕貞操
ELSEIF TALENT:31
	IF EXP:0 == 0
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 300
	ENDIF
ELSE
	IF EXP:0 == 0
		SOURCE:15 = 3000
	ENDIF
ENDIF

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:0 , 0.80
	TIMES SOURCE:3 , 0.60
ELSEIF PALAM:5 < PALAMLV:2
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:0 , 1.00
	TIMES SOURCE:3 , 1.00
ELSEIF PALAM:5 < PALAMLV:3
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:0 , 1.10
	TIMES SOURCE:3 , 1.50
ELSEIF PALAM:5 < PALAMLV:4
	TIMES SOURCE:1 , 1.40
	TIMES SOURCE:0 , 1.20
	TIMES SOURCE:3 , 2.00
ELSEIF PALAM:5 >= PALAMLV:4
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:0 , 1.30
	TIMES SOURCE:3 , 2.60
ENDIF

;ABL:順從をみる
IF ABL:10 == 0
	TIMES SOURCE:1 , 1.50
	TIMES SOURCE:3 , 1.00
	TIMES SOURCE:15 , 2.00
ELSEIF ABL:10 == 1
	TIMES SOURCE:1 , 1.50
	TIMES SOURCE:3 , 1.30
	TIMES SOURCE:15 , 1.80
ELSEIF ABL:10 == 2
	TIMES SOURCE:1 , 1.50
	TIMES SOURCE:3 , 1.50
	TIMES SOURCE:15 , 1.60
ELSEIF ABL:10 == 3
	TIMES SOURCE:1 , 1.80
	TIMES SOURCE:3 , 1.90
	TIMES SOURCE:15 , 1.40
ELSEIF ABL:10 == 4
	TIMES SOURCE:1 , 2.10
	TIMES SOURCE:3 , 2.20
	TIMES SOURCE:15 , 1.20
ELSE
	TIMES SOURCE:1 , 2.50
	TIMES SOURCE:3 , 2.60
	TIMES SOURCE:15 , 1.00
ENDIF

;プレイヤーのABL:技巧をみる
IF ABL:PLAYER:12 == 0
	SOURCE:3 += 100
	SOURCE:10 = 0
	A = 0
ELSEIF ABL:PLAYER:12 == 1
	SOURCE:3 += 150
	SOURCE:10 += 50
	A = 0
ELSEIF ABL:PLAYER:12 == 2
	SOURCE:3 += 200
	SOURCE:10 += 100
	A = 0
ELSEIF ABL:PLAYER:12 == 3
	SOURCE:3 += 300
	SOURCE:10 += 150
	A = 50
ELSEIF ABL:PLAYER:12 == 4
	SOURCE:3 += 500
	SOURCE:10 += 250
	A = 100
ELSE
	SOURCE:3 += 800
	SOURCE:10 += 400
	A = 300
ENDIF

SOURCE:0 += A

SIF EXPLV:10 >= 3
	SOURCE:1 += A

;TALENT:愛慕をみる
SIF TALENT:85
	TIMES SOURCE:3 , 2.00


;-----------------------------
;調教者點數
;REF eraH @COMF81
;-----------------------------
LOCAL = 100 + 10 * MIN(ABL:技巧 + ABL:PLAYER:技巧)
SOURCE:PLAYER:陰核快感 += 150 * LOCAL /100

	
;セックス後の処理
CALL COM_AFTER_VAGINA_SEX

RETURN 1
;
;
;
