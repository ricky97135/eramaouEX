;	166,悪女　167,ハート;ユニーク　168,スペード;ユニーク　169,ダイヤ;ユニーク　170,クラブ;ユニーク
;	171,村娘Ｂ;リリィ専用性格、ユニーク　172,知的　173,庇護者, 174,貴公子
;ARG:2	種族設定
;	12：人間　1：エルフ　2：人狼　3：吸血鬼　4：デュラハン　5：ドラゴン
;	6：天使　7：ダークエルフ　8：墮天使　9：魔族　10：ホビット　11：ドワーフ
;---------------------------------------------------------

SWAP A, ARG

;性別
SIF !EX_TALENT:A:2
	CALL CM_GENDER
	
;命名
SIF !EX_TALENT:A:2	;後代：命名->設定家族關係->CHARA_MAKE
	CALL CHARA_NAME_RANDOM_DEFINE(A)

;Level・經驗值設定
CFLAG:A:9 = 1
EXP:A:80 = 0

;家族初期化
CFLAG:A:605 = 0

;売春への積極性
CFLAG:A:120 = 1

IF !TALENT:A:精英 && !EX_TALENT:A:1 && !EX_TALENT:A:2
	;侵攻樓層
	CALL CM_STP
	;職業、基礎
	CALL CM_BASE
	;勇者初始等級
	CALL CM_ST
ELSEIF !EX_TALENT:A:2
	;初始位置
	CFLAG:A:1 = 0
	;職業、基礎
	CALL CM_BASE
	;精英部下初始等級
	CALL CM_ST_ACE
ELSE
	;初始位置
	CFLAG:A:1 = 0
	;職業、基礎
	CALL CM_BASE
ENDIF


;口上性格
IF INRANGE(NO:A,1,16)
	CALL CM_KJ, ARG:1
ELSEIF INRANGE(NO:A,200,211)
	;精英，暫用勇者口上
	CALL CM_KJ, ARG:1
ENDIF


;初心者の烙印
SIF DAY:0 <= 60
	TALENT:A:291 = 1

;處女
CALL CM_VIRGIN

;素質
CALL CM_TALENT

;戰術技能
CALL CM_SKILL

;外貌
CALL CM_LOOK, ARG:2
;2016/11/1 令後代來歷生效
IF EX_TALENT:A:2
	TALENT:A:陰毛狀態 = 2
	;TALENT:A:成為勇者前的生活 = 0
	;TALENT:A:成為勇者的契機 = 0
ENDIF
	
;善惡
CALL CM_KIND

;妊娠性交經驗
SIF !EX_TALENT:A:2
	CALL CM_NS_EXP

; [IF_DEBUG]
; ;家族設定（後代不設定家族
; SIF RAND:4 == 0 && !EX_TALENT:A:2
	; CALL SET_FAMILY_LINK, A

; 
; [ENDIF]

;新的家族系統（後代不設定家族
SIF RAND:4 == 0 && !EX_TALENT:A:2
	CALL FAMILY_REGISTER(A)
	
;根據家族成員繼承素質
CALL CM_FAMILY_TALENT
	

;コスチューム
CALL CM_CLOTH	;SET_CHAR_CLOTH

;一人稱の設定
CALL RANDOM_SELF_CALL, A

;年齢or身長などを表示する設定の場合は身體データを設定
IF GETBIT(FLAG:5,12) || GETBIT(FLAG:5,15)
	CALL CHAR_BODY_GENERATE_WAPPED, A
ENDIF

SWAP A, ARG
RETURN ARG


