@CM_KJ, ARG = 0
;性格設定
;160,慈愛,
;161,自信家,
;162,懦弱,
;163,高貴,
;164,冷靜,
;166,惡女,
;174,貴公子
VARSET TALENT:A:0, 0, 160, 180
IF INRANGE(ARG,160,180)
	TALENT:A:ARG = 1
ELSE
	$CHARA_MIND_LOOP
	X = RAND:9 + 160

	;ユニークは除外
	SIF X == 165
		GOTO CHARA_MIND_LOOP
	;オトコなら悪女なれない
	SIF TALENT:A:122 && X == 166
		GOTO CHARA_MIND_LOOP
	;高貴のオトコならきっと貴公子です
	SIF TALENT:A:122 && X == 163
		X = 174
	IF X >= 167 && X <= 169
		X += 5
		;女で貴公子？いえ、高貴の女だわ
		SIF TALENT:A:122 == 0 && X == 174
			X = 163
	
	ENDIF
	TALENT:A:X = 1
ENDIF



