@COM100
;觸手召喚

PRINTL 召喚觸手
;SAVESTR:22 = 觸手召喚
CALL TRAIN_MESSAGE_B

IF TEQUIP:90
	TEQUIP:90 = 0

	;觸手插入(バイブ)
	TEQUIP:11 = 0
	;肛交觸手(アナルバイブ)
	TEQUIP:13 = 0
	;觸手凌辱陰蒂(陰蒂夾)
	TEQUIP:14 = 0
	;觸手凌辱乳頭(乳頭夾)
	TEQUIP:15 = 0
	;觸手榨乳(搾乳器)
	TEQUIP:16 = 0
	;觸手凌辱陰莖(オナホ)
	TEQUIP:17 = 0
	;觸手緊縛(繩子)
	TEQUIP:44 = 0
	;觸手灌腸(浣腸器)
	TEQUIP:46 = 0
	;觸手口辱
	TEQUIP:98 = 0

ELSE
	TEQUIP:90 = 1

	A = 100
	IF EXP:55 < EXPLV:1
		TIMES A , 3.00
	ELSEIF EXP:55 < EXPLV:2 / 2
		TIMES A , 2.50
	ELSEIF EXP:55 < EXPLV:3 / 2
		TIMES A , 2.00
	ELSEIF EXP:55 < EXPLV:4 / 2
		TIMES A , 1.00
	ELSEIF EXP:55 < EXPLV:5 / 2
		TIMES A , 0.80
	ELSE
		TIMES A , 0.60
	ENDIF

	;膽怯
	SIF TALENT:10
		TIMES A , 2.00
	;感情淡薄
	SIF TALENT:22
		TIMES A , 0.60

	LOSEBASE:0 += A
	LOSEBASE:1 += A * 2

	UP:10 += A * 20
	SOURCE:14 += A * 5
ENDIF
T = 0
RETURN 1


@EQUIP_COM100
;觸手調教中
PRINTL ＜觸手調教中＞

A = 100
IF EXP:55 < EXPLV:1
	TIMES A , 3.00
ELSEIF EXP:55 < EXPLV:2
	TIMES A , 2.50
ELSEIF EXP:55 < EXPLV:3
	TIMES A , 2.00
ELSEIF EXP:55 < EXPLV:4
	TIMES A , 1.00
ELSEIF EXP:55 < EXPLV:5
	TIMES A , 0.80
ELSE
	TIMES A , 0.60
ENDIF

;膽怯
SIF TALENT:10
	TIMES A , 2.00

;感情淡薄
SIF TALENT:22
	TIMES A , 0.60

LOSEBASE:0 += A
LOSEBASE:1 += A * 2

UP:10 += A * 20
SOURCE:8 += A * 10
SOURCE:14 += A * 5

SOURCE:10 += 2000

TIMES SOURCE:0 , 2.00
TIMES SOURCE:1 , 2.00
TIMES SOURCE:2 , 2.00
TIMES SOURCE:17 , 2.00
TIMES SOURCE:13 , 1.80

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
IF MAXBASE:PLAYER:4 != 0

	B = 0
	;ABL:技巧をみる
	IF ABL:12 == 0
		B = 500
	ELSEIF ABL:12 == 1
		B = 600
	ELSEIF ABL:12 == 2
		B = 800
	ELSEIF ABL:12 == 3
		B = 1000
	ELSEIF ABL:12 == 4
		B = 1400
	ELSE
		B = 2000
	ENDIF

	;ABL:順從をみる
	IF ABL:10 == 0
		TIMES B , 0.80
	ELSEIF ABL:10 == 1
		TIMES B , 0.90
	ELSEIF ABL:10 == 2
		TIMES B , 1.00
	ELSEIF ABL:10 == 3
		TIMES B , 1.10
	ELSEIF ABL:10 == 4
		TIMES B , 1.20
	ELSE
		TIMES B , 1.30
	ENDIF

	;PALAM:欲情をみる
	IF PALAM:5 < PALAMLV:1
		TIMES B , 1.00
	ELSEIF PALAM:5 < PALAMLV:2
		TIMES B , 1.10
	ELSEIF PALAM:5 < PALAMLV:3
		TIMES B , 1.20
	ELSEIF PALAM:5 < PALAMLV:4
		TIMES B , 1.30
	ELSEIF PALAM:5 < PALAMLV:5
		TIMES B , 1.40
	ELSE
		TIMES B , 1.50
	ENDIF

	;觸手插入
	SIF TEQUIP:11
		TIMES B , 1.50
	;肛交觸手
	SIF TEQUIP:13
		TIMES B , 1.50
	;觸手凌辱陰蒂
	SIF TEQUIP:14
		TIMES B , 1.20
	;觸手凌辱乳頭
	SIF TEQUIP:15
		TIMES B , 1.20
	;觸手榨乳
	SIF TEQUIP:16
		TIMES B , 1.30
	;觸手凌辱陰莖
	SIF TEQUIP:17
		TIMES B , 1.10
	;觸手緊縛
	SIF TEQUIP:44
		TIMES B , 1.20
	;觸手灌腸
	SIF TEQUIP:46
		TIMES B , 1.30
	;觸手口辱
	SIF TEQUIP:98
		TIMES B , 1.50

	BASE:PLAYER:4 += B

	S = BASE:PLAYER:4
	EJAC = MAXBASE:PLAYER:4

	IF  S > EJAC * 2
		E = 2
	ELSEIF S > EJAC
		E = 1
	ELSE
		E = 0
	ENDIF

	;大量射精
	IF E == 2
		EXP:20 += 3
		PRINTL 觸手大量射精
		PRINTL 精液經驗＋３
		T += 1
	;觸手バイブで膣內射精
	SIF TEQUIP:11
		TFLAG:38 = 2

		BASE:PLAYER:4 -= EJAC*2
		SIF BASE:PLAYER:4 >= EJAC
			BASE:PLAYER:4 = EJAC-1

	;通常の射精
	ELSEIF E == 1
		EXP:20 += 1
		PRINTL 觸手射精
		PRINTL 精液經驗＋１
		T += 1
	;觸手バイブで膣內射精
	SIF TEQUIP:11
		TFLAG:38 = 1

		BASE:PLAYER:4 -= EJAC
		SIF BASE:PLAYER:4 >= EJAC
			BASE:PLAYER:4 = EJAC-1
	ENDIF

;觸手が射精フラグ
TFLAG:15 = E

ENDIF

IF EXP:55 == 0
	EXP:50 += 1
	PRINTL 異常經驗＋1
ENDIF

T += 1
PRINT 觸手經驗＋
PRINTVL T
EXP:55 += T
T = 0
RETURN 1


