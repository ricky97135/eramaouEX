@ENEMY_DATA_CHECK, ARG:0, ARG:1
#DIM INUM
#DIM TOP

IF INUM >= 1000 && INUM <= 1009
	CALL CRUSADER, INUM, TOP
ELSE
ENDIF

;---------------------------------------------------------
@CRUSADER, ARG:0, ARG:1
;---------------------------------------------------------
#DIM INUM
#DIM TOP
;---------------------------------------------------------
;十字軍シリーツ

;番號
E:TOP = INUM

;レベル
E:(TOP+1) = 15

;攻撃力
E:(TOP+2) = 20

;防禦力
E:(TOP+3) = 25

;速度
E:(TOP+4) = 0

;特殊能力
E:(TOP+5) = 0

;魔法
E:(TOP+6) = 0

;陵辱タイプ
E:(TOP+7) = 0

;耐性(無し)
E:(TOP+10) = 0

RETURN 0






















