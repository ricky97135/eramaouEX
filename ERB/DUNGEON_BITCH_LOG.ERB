;売春ログ表示文字列関數
;プレイ回數や客の合計を出力
@FS_LOG_BITCH(TYPE, ARG:1, ARG:2, ARG:3, ARG:4, ARG:5)
#FUNCTIONS
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS TYPE
#DIM LCOUNT
VARSET LOCALS
SELECTCASE TYPE
CASE "DUNGEON_MAN", "DUNGEON_GIRL", "TOWN_MAN", "TOWN_GIRL"
	FOR LCOUNT, 1, 6
		SIF !ARG:LCOUNT
			CONTINUE
		SIF STRLENS(LOCALS)
			LOCALS += "、"
		LOCALS += @"{ARG:LCOUNT}人的%FS_BITCH(TYPE, LCOUNT)%"
	NEXT
CASE "PLAYNAME"
	FOR LCOUNT, 1, 6
		SIF !ARG:LCOUNT
			CONTINUE
		SIF STRLENS(LOCALS)
			LOCALS += "、"
		LOCALS += @"{ARG:LCOUNT}次的%FS_BITCH("PLAYNAME", LCOUNT)%"
	NEXT
CASEELSE
	THROW 未知的TYPE%TYPE%
ENDSELECT
RETURNF LOCALS



;売春描寫関數呼び出し関數
;売春が全て終わって（成功して）から呼ぶ
;BIT利用で売春の內容を記録した引數CHECKを元に描寫分岐を行う
;0 　　　=TOWN/DUNGEON
;1～6　　プレイ內容（1=HAND, 2=ORAL, 3=LES, 4=ANAL, 5=SEX, 6=ANIMAL）
;11～15　男性客の種類
;21～25　女性客の種類
;ARG＝キャラ登録番號はこの関數では使用しないが描寫関數側で必要となるためそのまま渡す
@LOG_AFTER_BITCH(ARG, CHECK)
#LOCALSIZE 1
#LOCALSSIZE 1
#DIM CHECK
#DIM KYAKU
#DIM PLAY
;けも
IF GETBIT(CHECK, 6)
	PLAY = 6
;れず
ELSEIF GETBIT(CHECK, 3) && RAND:10 < 5
	DO
		KYAKU = RAND(1, 6)
		SIF GETBIT(CHECK, (KYAKU + 20))
			BREAK
	LOOP 1
	PLAY = 3
;ふつう
ELSE
	;デバッグ加筆
	;恐らくレズ設定でRAND判定から外れてこっちに來た時に
	;女性客を取りたいのに男性客から判定しているため
	;無限ループが発生していると解釈して
	;処置を行いました
	IF GETBIT(CHECK, 3)
		DO
			KYAKU = RAND(1, 6)
			SIF GETBIT(CHECK, (KYAKU + 20))
				BREAK
		LOOP 1
		PLAY = 3
	ELSE
		DO
			KYAKU = RAND(1, 6)
			SIF GETBIT(CHECK, (KYAKU + 10))
				BREAK
		LOOP 1
		DO
			PLAY = RAND(1, 6)
			SIF PLAY == 3
				CONTINUE
			SIF GETBIT(CHECK, PLAY)
				BREAK
		LOOP 1
	ENDIF
	
ENDIF

LOCALS = %FS_BITCH("PLAY", PLAY)%
IF GETBIT(CHECK, 0)
	CALLFORM LOG_BITCH_%LOCALS%(ARG, "DUNGEON", KYAKU)
ELSE
	CALLFORM LOG_BITCH_%LOCALS%(ARG, "TOWN", KYAKU)
ENDIF
WAIT


