	ELSEIF MONTYPE == 11
		;脳姦
		CALL BRAIN_QUEST_BITCH,ARG,MCOUNT
	ELSEIF MONTYPE == 12
		;馬
		CALL HORSE_QUEST_BITCH,ARG,MCOUNT
	ENDIF
	
NEXT

IF EXP:0 > 0 && TALENT:0 == 1
	TALENT:0 = 0
	PRINTL 【処女喪失】
	CFLAG:15 = 104
ENDIF


RETURN 1

