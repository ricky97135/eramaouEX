		SOURCE:12 += 10000
		SOURCE:13 += 5000
	ELSEIF EXP:54 < EXPLV:2
		SOURCE:12 += 5000
		SOURCE:13 += 4000
	ELSEIF EXP:54 < EXPLV:3
		SOURCE:12 += 2500
		SOURCE:13 += 2000
	ELSEIF EXP:54 < EXPLV:4
		SOURCE:12 += 1600
		SOURCE:13 += 1400
	ELSEIF EXP:54 < EXPLV:5
		SOURCE:12 += 800
		SOURCE:13 += 500
	ELSE
		SOURCE:12 += 200
		SOURCE:13 += 250
	ENDIF

	PRINTFORML %SAVESTR:TARGET%的乳頭流出了母乳。
	PRINTL 噴奶經驗＋１
	IF EXP:54 == 0
		EXP:50 += 1
		PRINTL 異常經驗＋1
	ENDIF
	EXP:54 += 1
	;Ｂに母乳汚れ
	STAIN:5 |= 16

	BASE:3 -= EJAC
	SIF BASE:3 >= EJAC
