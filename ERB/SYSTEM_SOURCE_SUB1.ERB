	;反抗刻印迴避
	TFLAG:150 = 1
ENDIF
;-------------------------------------------------
;調教対象の射精チェック
;-------------------------------------------------
@TARGET_EJAC_CHECK
#DIM MIJYUKU
#DIM MIJYUKU_KENKAI
MIJYUKU = 0
MIJYUKU_KENKAI = BASE:2
SIF TALENT:121 == 0 && TALENT:122 == 0
	RETURN 0
SIF TALENT:135
	MIJYUKU = RAND:700 - RAND:800 + 400
	;RETURN 0

LOCAL = UP:0 + UP:1 + UP:2 + UP:14

;克制
SIF TALENT:20
	LOCAL /= 2

;接受快感
SIF TALENT:70
	TIMES LOCAL , 1.20

;淫亂化
SIF TALENT:76
	TIMES LOCAL , 1.10

;否定快感
SIF TALENT:71
	TIMES LOCAL , 0.80

;媚藥
SIF TEQUIP:21
	LOCAL *= 2

;利尿劑
SIF TEQUIP:22
	LOCAL /= 2

;調教対象が安全套裝著
SIF TEQUIP:37
	LOCAL /= 2
;未熟
SIF TALENT:135
	LOCAL -= MIJYUKU
	
LOCAL = 1000+(LOCAL-1000)/2
BASE:2 += LOCAL
SIF TALENT:135 && MIJYUKU_KENKAI < 2000 && BASE:2 >= 2000
	PRINTFORML %SAVESTR:TARGET%尚未成熟的陰莖似乎漸漸有了感覺。
SIF TALENT:135 && MIJYUKU_KENKAI >= 2000 && BASE:2 <= 2000
	BASE:2 = 2000
LOCAL = BASE:2
EJAC = MAXBASE:2

IF  LOCAL > EJAC * 2
	LOCAL:1 = 2
ELSEIF LOCAL > EJAC
	LOCAL:1 = 1
ELSE
	LOCAL:1 = 0
ENDIF

;大量射精
IF LOCAL:1 == 2
	LOSEBASE:0 += 20
	LOSEBASE:1 += 100

	IF EXP:3 < EXPLV:1
		SOURCE:12 += 20000
		SOURCE:13 += 10000
	ELSEIF EXP:3 < EXPLV:2
		SOURCE:12 += 10000
		SOURCE:13 += 8000
	ELSEIF EXP:3 < EXPLV:3
		SOURCE:12 += 7000
		SOURCE:13 += 6000
	ELSEIF EXP:3 < EXPLV:4
		SOURCE:12 += 5000
		SOURCE:13 += 4000
	ELSEIF EXP:3 < EXPLV:5
		SOURCE:12 += 3000
		SOURCE:13 += 2000
	ELSE
		SOURCE:12 += 1800
		SOURCE:13 += 1200
	ENDIF

	PRINTS SAVESTR:TARGET
	PRINTL 大量射精
	PRINTL 精液經驗＋１
	PRINTL 射精經驗＋２
	IF EXP:3 == 0 && TALENT:122 == 0
		EXP:50 += 1
		PRINTL 異常經驗＋1
	ENDIF
	EXP:20 += 1
	EXP:3 += 2
	
	;未熟
	IF TALENT:135
		MAXBASE:0 -= 10
		MAXBASE:1 -= 30
		LOSEBASE:0 += 50
		LOSEBASE:1 += 100
		PRINTFORML %SAVESTR:TARGET%感到身體被掏空。
		PRINTFORML 體力上限下降了10。
		PRINTFORML 氣力上限下降了30。
		;最大體力は600まで下がる（それ以下にはならない）
		SIF MAXBASE:0 < 600
			MAXBASE:0 = 600
		;最大気力は100まで下がる（それ以下にはならない）
		SIF MAXBASE:1 < 100
			MAXBASE:1 = 100
	ENDIF
		
	;Ｐに精液汚れ
	STAIN:2 |= 4

	BASE:2 -= EJAC*2
	SIF BASE:2 >= EJAC
		BASE:2 = EJAC-1

	;調教対象が射精フラグ
	TFLAG:10 = 2
	NOWEX:5 += 1
	EX:5 += 1
;通常の射精
ELSEIF LOCAL:1 == 1
	LOSEBASE:1 += 40

	IF EXP:3 < EXPLV:1
		SOURCE:12 += 10000
		SOURCE:13 += 5000
	ELSEIF EXP:3 < EXPLV:2
		SOURCE:12 += 5000
		SOURCE:13 += 4000
	ELSEIF EXP:3 < EXPLV:3
		SOURCE:12 += 2500
		SOURCE:13 += 2000
	ELSEIF EXP:3 < EXPLV:4
		SOURCE:12 += 1600
		SOURCE:13 += 1400
	ELSEIF EXP:3 < EXPLV:5
		SOURCE:12 += 800
		SOURCE:13 += 500
	ELSE
		SOURCE:12 += 200
		SOURCE:13 += 250
	ENDIF

	PRINTS SAVESTR:TARGET
	PRINTL 射精
	PRINTL 射精經驗＋１
	IF EXP:3 == 0 && TALENT:122 == 0
		EXP:50 += 1
		PRINTL 異常經驗＋1
	ENDIF
	EXP:3 += 1

	;未熟
	IF TALENT:135
		MAXBASE:0 -= 10
		MAXBASE:1 -= 10
		LOSEBASE:0 += 10
		LOSEBASE:1 += 40
		PRINTFORML %SAVESTR:TARGET%感到身體一陣空虛。
		PRINTFORML 體力和氣力上限下降了10。
	ENDIF
	
	;Ｐに精液汚れ
	STAIN:2 |= 4

	BASE:2 -= EJAC
	SIF BASE:2 >= EJAC
		BASE:2 = EJAC-1

	;調教対象が射精フラグ
	TFLAG:10 = 1
	NOWEX:6 += 1
	EX:6 += 1
ENDIF

