PRINTLC [1000] - 上一頁
PRINTLC [999] - 返  回
PRINTLC [1001] - 下一頁
PRINTL 

INPUT
SIF RESULT == 0
	RESULT = MASTER
IF RESULT == 999
	RETURN 0
ELSEIF RESULT == 1000		;上一頁
	IF NO_PAGE > 0
		NO_PAGE --
		CLEARLINE LINECOUNT-L_LCOUNT		
	ENDIF
	GOTO INPUT_LOOP
ELSEIF RESULT == 1001		;下一頁
	IF (NO_PAGE+1) * NUM_PAGE <= CHARANUM
		NO_PAGE ++
		CLEARLINE LINECOUNT-L_LCOUNT
	ENDIF
	GOTO INPUT_LOOP
ELSEIF RESULT < 0 || RESULT >= CHARANUM
	GOTO INPUT_LOOP
;臨死中のキャラは排除
ELSEIF BASE:RESULT:0 < 1
	GOTO INPUT_LOOP
ELSEIF TALENT:RESULT:122
	PRINTFORMW 【%TALENTNAME:122%】不能巨乳化。
	RETURN 0
ELSEIF TALENT:RESULT:119
	PRINTFORMW 知足吧，%SAVESTR:RESULT%的胸，已經是奇尺大乳了。
	RETURN 0
ENDIF

SIF RESULT == 1001 || RESULT == 1000
	GOTO INPUT_LOOP

IF TALENT:RESULT:110 || TALENT:RESULT:114
	PRINTFORML %SAVESTR:RESULT%胸部偉岸，要更上一層樓，需要50000點。
	C = 50000
	SIF MONEY < C
		GOTO INPUT_LOOP
	PRINTL 這樣還要繼續么？
ELSE
	PRINTFORML 為%SAVESTR:RESULT%進行巨乳化改造嗎？
ENDIF

T = RESULT

PRINTL  [0] - 好的
PRINTL  [1] - 不要

INPUT

IF RESULT == 1
	RETURN 0
ELSEIF RESULT == 0 && TALENT:T:116
	PRINTFORMW 《%SAVESTR:T%的【%TALENTNAME:109%】消去了》
	TALENT:T:116 = 0
	TALENT:T:109 = 1
ELSEIF RESULT == 0 && TALENT:T:109
	PRINTFORMW 《%SAVESTR:T%的胸是標準大小》
	TALENT:T:109 = 0
ELSEIF RESULT == 0 && TALENT:T:110
	PRINTFORMW 《%SAVESTR:T%獲得【%TALENTNAME:114%】了》
	TALENT:T:110 = 0
	TALENT:T:114 = 1
ELSEIF RESULT == 0 && TALENT:T:114
	PRINTFORMW 《%SAVESTR:T%獲得【%TALENTNAME:119%】了》
	TALENT:T:114 = 0
	TALENT:T:119 = 1
ELSEIF RESULT == 0 
	PRINTFORMW 《%SAVESTR:T%獲得【%TALENTNAME:110%】了》
	TALENT:T:110 = 1
ELSE
	RETURN 0
ENDIF

MONEY -= C
EX_FLAG:4444 -= C
;対象があなたでなく、コンフィグ設定してあるならバストサイズ更新
IF T != 0
	IF GETBIT(FLAG:5,12) || GETBIT(FLAG:5,15)
		;CALL CHAR_BUST_REGENERATE_WAPPED, T
		CALL CHAR_SIZE_GENERATE, T, CFLAG:T:451, 1
		CFLAG:T:454 = RESULT:3
		CFLAG:T:455 = RESULT:4
	ENDIF
ENDIF

RETURN 1
